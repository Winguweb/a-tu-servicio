development: &app_config
  algolia:
    admin_api_key: <%= ENV['ALGOLIA_ADMIN_KEY'] %>
    application_id: <%= ENV['ALGOLIA_APP_KEY'] %>
    search_api_key: <%= ENV['ALGOLIA_SEARCH_KEY'] %>

  google:
    analytics:
    geocoding:
      api_key:
    recaptcha:
      secretkey:
      sitekey: <%= ENV['GOOGLE_RECAPTCHA_SITE_KEY'] %>
      verifyUrl:

  mapbox:
    token: <%= ENV['MAPBOX_TOKEN'] %>

  redis:
    host: 'localhost'
    port: 6379
    db:   0

  vuejs:
    cdn_url: <%= ENV['VUEJS_CDN'] %>

production:
  <<: *app_config

test:
  <<: *app_config